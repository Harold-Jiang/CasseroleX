@inject ICurrentUserService _currentUserService
<div class="panel panel-default panel-intro">
    @await Component.InvokeAsync("BuildHeading")
    <div class="panel-body">
        <div id="myTabContent" class="tab-content">
            <div class="tab-pane fade active in" id="one">
                <div class="widget-body no-padding">
                    <div id="toolbar" class="toolbar">
                        @await Component.InvokeAsync("BuildToolbar",new []{ "refresh","add","del" })
                    </div>
                    <table id="table" class="table table-striped table-bordered table-hover table-nowrap"
                           data-operate-edit="@(await _currentUserService.CheckPermissionAsync("auth/admin/edit"))"
                           data-operate-del="@(await _currentUserService.CheckPermissionAsync("auth/admin/del"))"
                           width="100%">
                    </table>
                </div>
            </div>

        </div>
    </div>
</div>
